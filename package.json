{
  "name": "bookbag-ce",
  "version": "0.0.14",
  "private": true,
  "scripts": {
    "init-jwt": "node scripts/init-jwt-secrets.js",
    "init-jwt:dev": "node scripts/init-jwt-secrets.js development",
    "init-jwt:prod": "node scripts/init-jwt-secrets.js production",
    "update-cors": "node scripts/update-cors.js",
    "dev": "npm run init-jwt:dev && master=development node server.js",
    "prod": "npm run init-jwt:prod && master=production node server.js",
    "deploy": "bash deploy.sh",
    "deploy:windows": "powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "start:backend": "npm run init-jwt:prod && master=production node server.js",
    "start:backend:dev": "npm run init-jwt:dev && master=development node server.js",
    "deploy:dev": "pm2 start ecosystem.dev.config.js",
    "deploy:prod": "pm2 start ecosystem.prod.config.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:dev": "pm2 start ecosystem.dev.config.js",
    "pm2:start:prod": "pm2 start ecosystem.prod.config.js",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "bcryptjs": "^3.0.2",
    "content-type": "^1.0.5",
    "jsonwebtoken": "^9.0.2",
    "mastercontroller": "^1.2.13",
    "masterrecord": "^0.2.33",
    "nodemailer": "^7.0.6",
    "object-mapper": "^6.2.0",
    "openai": "^5.16.0",
    "promise": "^8.3.0",
    "qs": "^6.14.0",
    "socket.io": "^4.8.1",
    "sweetalert2": "^11.23.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "esbuild": "^0.25.12",
    "eslint": "^9.36.0"
  },
  "type": "commonjs"
}
