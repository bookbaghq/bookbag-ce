{
    "origin": ["http://localhost:3000"],
    "methods": [
        "GET",
        "HEAD",
        "POST",
        "DELETE",
        "PUT",
        "PATCH",
        "OPTIONS"
    ],
    "allowedHeaders": [
        "Content-Type",
        "Authorization",
        "X-Requested-With",
        "Accept",
        "Origin",
        "Cookie"
    ],
    "exposeHeaders": [
        "Set-Cookie"
    ],
    "credentials": true,
    "maxAge": 3600
}